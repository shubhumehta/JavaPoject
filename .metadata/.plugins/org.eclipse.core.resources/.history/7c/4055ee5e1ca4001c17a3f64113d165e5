package com.capg.practice;

public class Constructor {

	static int a ;
	
	static int b;

	 public  Constructor(){
		  a = 5;
		 	b = 3;
			 
		 }
		 
	
	public static void method() {
		 System.out.println(a*b);
	 }
	
	
	public static void main(String args[]) {
		
//		Constructor a = new Constructor();
		method();
//		method();
	}
}
