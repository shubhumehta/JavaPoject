package com.capg.practice;

public class Constructor {

//	int a = 100;
//	
//	int b= 2;

	 public  Constructor(){
		 int   a = 5;
		 int	b = 3;
			 
		 }
		 
	
	public void method() {
		 System.out.println(a*b);
	 }
	
	
	public static void main(String args[]) {
		
		Constructor a = new Constructor();
		a.method();
//		method();
	}
}
